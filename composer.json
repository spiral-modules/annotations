{
    "name": "spiral/annotations",
    "type": "library",
    "description": "Annotations parser using strict node grammar",
    "license": "MIT",
    "homepage": "https://spiral.dev",
    "support": {
        "issues": "https://github.com/spiral/framework/issues",
        "source": "https://github.com/spiral/annotations"
    },
    "require": {
        "php": ">=7.2",
        "spiral/tokenizer": "^2.7",
        "doctrine/annotations": "^1.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Spiral\\Annotations\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spiral\\Tests\\Annotations\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.7.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
